[
	{
		"bindings": {
			"cmd-1": null,
			"cmd-2": null,
			"ctrl-n": null,
			"ctrl-j": null,
			"ctrl-k": null,
			"ctrl-l": null,
			"shift-l": null,
			"cmd-t": null,
			"cmd-j": null,
			"g t": null
			// "cmd-a": null
		}
	},
	{
		"bindings": {
			"cmd-b": "workspace::ToggleLeftDock",
			"cmd-n": "workspace::ToggleRightDock",
			"cmd-1": "pane::ActivatePreviousItem",
			"cmd-2": "pane::ActivateNextItem",
			"ctrl-j": "menu::SelectNext",
			"ctrl-k": "menu::SelectPrevious",
			"cmd-j": "menu::SelectNext",
			"cmd-k": "menu::SelectPrevious",
			"ctrl-p": "file_finder::Toggle",
			"shift-cmd-t": "project_symbols::Toggle"
		}
	},

	{
		"context": "Editor && (showing_code_actions || showing_completions)",
		"bindings": {
			"up": "editor::ContextMenuPrevious",
			"down": "editor::ContextMenuNext",
			"ctrl-k": "editor::ContextMenuPrevious",
			"ctrl-j": "editor::ContextMenuNext"
		}
	},
	{
		"context": "Editor && !VimWaiting && !menu && vim_mode == normal",
		"bindings": {
			"ctrl-d": "editor::GoToDiagnostic",
			"ctrl-c": "editor::ToggleComments",

			"shift-k": "editor::Hover",
			"shift-d": "editor::DeleteLine",

			"g a": null,
			"g n": "editor::Rename",
			"g f": "editor::ToggleCodeActions",
			"g r": "editor::FindAllReferences",
			"d d": "editor::DeleteLine",
			"g y": "editor::GoToTypeDefinition",
			"g d": "editor::GoToDefinition",

			"9": "editor::MoveToEndOfLine",
			"8": "vim::Matching",
			"x": "editor::Delete",

			"(": ["editor::MoveUpByLines", { "lines": 15 }],
			")": ["editor::MoveDownByLines", { "lines": 15 }],
			"<": "editor::Outdent",
			">": "editor::Indent",

			"cmd-t": ["workspace::SendKeystrokes", "g s"],

			"d": "editor::Delete",
			// "c": "editor::Delete",

			"space": null,
			"Enter": null,

			"ctrl-b": null,
			"ctrl-e": null,
			"ctrl-u": null,
			"ctrl-v": null,
			"ctrl-z": null,
			"ctrl-t": null,
			"ctrl-f":null,

			"#": null,
			"-": null,
			"m": null,
			"M": null,
			"f": null,
			"F": null,
			"s": null,
			"S": null,
			"J": null,

			"1": null,
			"2": null,
			"3": null,
			"4": null,
			"5": null,
			"6": null,
			"7": null,

			"H":null,
			"L":null
		}
	},
	{
		"context": "Editor && !VimWaiting && !menu && vim_mode == visual",
		"bindings": {
			"ctrl-c": "editor::ToggleComments",

			"8": "vim::Matching",
			"d": "editor::Delete",
			"x": "editor::Delete",

			"g a": null,
			"g n": null,
			"g f": null,
			"g r": null,
			"g y": null,

			"9": "editor::SelectToEndOfLine",

			"(": ["editor::SelectUpByLines", { "lines": 15 }],
			")": ["editor::SelectDownByLines", { "lines": 15 }],

			"<": "editor::Outdent",
			">": "editor::Indent",

			"ctrl-f":null,
			"H":null,
			"L":null,

			"cmd-v": ["vim::Paste", { "preserve_clipboard": true }],
			"p": ["vim::Paste", { "preserve_clipboard": true }],
		}
	},
	{
		"context": "Editor && !VimWaiting && !menu && vim_mode == insert",
		"bindings": {
			"tab": "editor::ShowCompletions"
		}
	},
	{
		"context": "Editor && (vim_mode == normal || vim_mode == visual)",
		"bindings": {
			"ctrl-r": "vim::Redo",
			"u": "vim::Undo"
		}
	}
]
